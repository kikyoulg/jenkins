pipeline {
  agent any
  stages {
    stage('git clone') {
      steps {
        sh 'echo "hello"'
        git 'git@gitlab.datacanvas.com:fedx/fedx-apis.git'
      }
    }

    stage('build') {
      steps {
        sh 'echo "hello"'
      }
    }

    stage('push') {
      steps {
        sh 'echo "hello"'
      }
    }

    stage('deploy') {
      steps {
        sh 'echo "hello"'
      }
    }

  }
}